  <!-- ########## AtileHD ########## -->
  <!-- AtileHD Config -->
  <screen name="AtileHD_Config" position="0,0" size="1920,1080" title="AtileHD Setup" flags="wfNoBorder" backgroundColor="transparent">
    <panel name="TemplatesMenuLayer" />
    <panel name="TemplatesMenu1000Layer" />
    <panel name="TemplatesMenuLeftLayer" />
    <eLabel text="s" position="105,850" size="220,180" font="Icons; 110" foregroundColor="color5" backgroundColor="background2" halign="center" valign="center" transparent="1" />
    <panel name="TemplatesTitle" />
    <widget name="config" position="90,200" size="980,635" scrollbarMode="showNever" itemHeight="35" font="Global; 25" backgroundColor="background1" backgroundColorSelected="background5" transparent="1" />
    <panel name="TemplatesButtonsRedName" />
    <panel name="TemplatesButtonsGreenName" />
    <panel name="TemplatesButtonsYellowName" />
    <panel name="TemplatesButtonsBlueName" />
    <panel name="TemplatesButtons" />
    <panel name="TemplatesClock" />
  </screen>
  <!-- AtileHD Screens -->
  <screen name="AtileHDScreens" position="0,0" size="1920,1080" title="AtileHD Setup" flags="wfNoBorder" backgroundColor="transparent">
    <panel name="TemplatesMenuLayer" />
    <panel name="TemplatesMenu1000Layer" />
    <panel name="TemplatesMenuLeftLayer" />
    <eLabel text="s" position="105,850" size="220,180" font="Icons; 110" foregroundColor="color5" backgroundColor="background2" halign="center" valign="center" transparent="1" />
    <panel name="TemplatesTitle" />
    <widget source="menu" render="Listbox" position="90,200" size="480,600" scrollbarMode="showNever" backgroundColor="background1" backgroundColorSelected="background5" transparent="1">
    <convert type="TemplatedMultiContent">
			{"template": [
				MultiContentEntryText(pos=(10,2), size=(320, 35), text = 1, backcolor = MultiContentTemplateColor(3)),
				MultiContentEntryText(pos=(330,2), size=(150, 35), text = 2, flags = RT_HALIGN_CENTER, backcolor = MultiContentTemplateColor(3)) ],
			 "fonts": [gFont("Global", 25)],
			 "itemHeight": 35
			}
    </convert>
    </widget>
    <panel name="TemplatesButtonsRed" />
    <panel name="TemplatesButtonsGreen" />
    <panel name="TemplatesButtons" />
    <panel name="TemplatesClock" />
  </screen>
  <!-- ########## CyberFHD ########## -->
