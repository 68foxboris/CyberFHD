	<!-- ########## SkinLIB ########## -->
	<!-- Down SkinLIB -->
	<screen name="DownSkinLIB" position="fill" title="Download SkinLIB" backgroundColor="transparent" flags="wfNoBorder">
		<panel name="TemplatesMenu1000Layer" />
		<eLabel text="¤" position="105,850" size="220,180" font="Icons; 110" foregroundColor="color5" backgroundColor="background2" halign="center" valign="center" transparent="1" />
		<panel name="TemplatesTitle" />
		<widget name="info_conv_l" position="90,200" size="320,600" font="Global; 20" foregroundColor="color2" backgroundColor="background1" halign="left" transparent="1" />
		<widget name="info_conv_r" position="420,200" size="320,600" font="Global; 20" foregroundColor="color2" backgroundColor="background1" halign="left" transparent="1" />
		<widget name="info_rend" position="750,200" size="320,600" font="Global; 20" foregroundColor="color2" backgroundColor="background1" halign="left" transparent="1" />
		<widget name="info_git" position="340,895" size="1000,60" font="Global; 25" foregroundColor="color3" backgroundColor="background1" halign="left" valign="top" transparent="1" />
		<widget name="info_pl" position="340,960" size="1000,30" font="Global; 25" foregroundColor="color3" backgroundColor="background1" halign="left" valign="center" transparent="1" />
		<eLabel text="&gt;INFO&lt;" position="1485,100" size="120,30" font="Indication; 30" foregroundColor="color4" backgroundColor="background" halign="center" transparent="1" />
		<panel name="TemplatesButtonsKey" />
		<panel name="TemplatesClock" />
	</screen>
	<!-- ########## AtileHD ########## -->
	<!-- AtileHD Config -->
	<screen name="AtileHD_Config" position="fill" title="AtileHD Setup" backgroundColor="transparent" flags="wfNoBorder">
		<panel name="TemplatesMenu1000Layer" />
		<eLabel text="s" position="105,850" size="220,180" font="Icons; 110" foregroundColor="color5" backgroundColor="background2" halign="center" valign="center" transparent="1" />
		<panel name="TemplatesTitle" />
		<widget name="config" position="90,200" size="980,630" scrollbarMode="showNever" scrollbarWidth="10" scrollbarSliderForegroundColor="background4" scrollbarSliderBorderColor="background3" itemHeight="35" font="Global; 25" backgroundColor="background1" backgroundColorSelected="background5" transparent="1" />
		<panel name="TemplatesButtonsKey" />
		<panel name="TemplatesClock" />
	</screen>
	<!-- AtileHD Screens -->
	<screen name="AtileHDScreens" position="fill" title="AtileHD Setup" flags="wfNoBorder" backgroundColor="transparent">
		<panel name="TemplatesMenu1000Layer" />
		<eLabel text="s" position="105,850" size="220,180" font="Icons; 110" foregroundColor="color5" backgroundColor="background2" halign="center" valign="center" transparent="1" />
		<panel name="TemplatesTitle" />
		<widget source="menu" render="Listbox" position="90,200" size="480,600" scrollbarMode="showNever" scrollbarWidth="10" scrollbarSliderForegroundColor="background4" scrollbarSliderBorderColor="background3" backgroundColor="background1" backgroundColorSelected="background5" transparent="1">
		<convert type="TemplatedMultiContent">
			{"template": [
				MultiContentEntryText(pos=(10,2), size=(320, 35), text = 1, backcolor = MultiContentTemplateColor(3)),
				MultiContentEntryText(pos=(330,2), size=(150, 35), text = 2, flags = RT_HALIGN_CENTER, backcolor = MultiContentTemplateColor(3))],
				"fonts": [gFont("Global", 25)],
				"itemHeight": 35}
		</convert>
		</widget>
		<panel name="TemplatesButtonsKey" />
		<panel name="TemplatesClock" />
	</screen>
